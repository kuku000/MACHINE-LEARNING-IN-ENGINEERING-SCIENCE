   "source": [
    "import math\n",
    "\n",
    "def sin(x):\n",
    "    #25次\n",
    "    N = 25\n",
    "    sum = 0\n",
    "    #泰勒展開\n",
    "    for i in range(1,N+1):\n",
    "        sum += (-1)**(i - 1)*x**(2*i - 1)/math.factorial(2*i - 1)\n",
    "    return sum\n",
    "        "
   ]
  },
